{
  "name": "frontend-booster-react-spa",
  "version": "1.0.0",
  "description": "React SPA boilerplate",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "prod": "cross-env NODE_ENV=production serve -n -s build -p ${PORT:-3002}",
    "dev": "cross-env NODE_ENV=development webpack serve --port ${PORT:-3000}",
    "test": "cross-env NODE_ENV=test npm run test:unit && npm run test:integration:headless",
    "test:unit": "cross-env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"CommonJS\" }' mocha",
    "test:integration": "cross-env NODE_ENV=test PORT=${PORT:-3001} start-server-and-test dev http://localhost:${PORT:-3001} 'cypress open'",
    "test:integration:headless": "cross-env NODE_ENV=test PORT=${PORT:-3001} start-server-and-test dev http://localhost:${PORT:-3001} 'cypress run'"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "serve": "^12.0.1",
    "style-loader": "^3.3.0",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/faker": "^5.5.8",
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@types/sinon": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "chai": "^4.3.4",
    "cypress": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "jsdom": "17.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "prettier": "^2.4.1",
    "sinon": "^11.1.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",

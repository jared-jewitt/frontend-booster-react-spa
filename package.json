{
  "name": "frontend-booster-react-spa",
  "version": "1.0.0",
  "description": "React SPA boilerplate",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "webpack",
    "prod": "cross-env NODE_ENV=production sh env.sh && serve -n -s build -p ${PORT:-4002}",
    "dev": "cross-env NODE_ENV=development PORT=${PORT:-4000} webpack serve",
    "test": "cross-env NODE_ENV=test PORT=${PORT:-4001} npm run test:unit && npm run test:e2e:headless",
    "test:e2e": "cross-env NODE_ENV=test PORT=${PORT:-4001} start-server-and-test dev http://localhost:${PORT:-4001} 'cypress open'",
    "test:e2e:headless": "cross-env NODE_ENV=test PORT=${PORT:-4001} start-server-and-test dev http://localhost:${PORT:-4001} 'cypress run'",
    "test:unit": "cross-env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{ \"module\": \"CommonJS\" }' mocha -r dotenv-flow/config -r ts-node/register -r tsconfig-paths/register -r jsdom-global/register"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/chai": "^4.2.22",
    "@types/faker": "^5.5.9",
    "@types/mocha": "^9.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cypress": "^8.7.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "18.0.1",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^2.4.4",
    "mocha": "^9.1.3",
    "nprogress": "^0.2.0",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "serve": "^13.0.2",
    "sinon": "^12.0.1",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-standard-scss": "^2.0.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "webpack-dev-server": "^4.4.0"
  }
}
